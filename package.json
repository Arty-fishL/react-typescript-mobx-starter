{
  "name": "react-webpack-typescript-starter",
  "version": "0.1.0",
  "description": "Starter kit for React, Webpack (with Hot Module Replacement), Typescript and MobX.",
  "keywords": [
    "react",
    "webpack",
    "typescript",
    "mobx",
    "babel",
    "sass",
    "hmr",
    "starter",
    "boilerplate"
  ],
  "author": "Jamie Lievesley",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Arty-fishL/react-typescript-mobx-starter.git"
  },
  "bugs": {
    "url": "https://github.com/Arty-fishL/react-typescript-mobx-starter/issues"
  },
  "homepage": "https://github.com/Arty-fishL/react-typescript-mobx-starter",
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "scripts": {
    "build": "yarn run clean && webpack -p --config=configs/webpack/prod.js",
    "clean": "rimraf dist/*",
    "lint": "tslint './src/**/*.ts*' --format stylish --project . --force",
    "lint:fix": "yarn run lint --fix",
    "start": "yarn run start:dev",
    "start:dev": "webpack-dev-server --config=configs/webpack/dev.js",
    "start:prod": "yarn run build && node express.js",
    "test": "jest --coverage --watchAll --config=configs/jest.json"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^25.1.2",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.7.1",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "express": "^4.17.1",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^6.0.0",
    "jest": "^25.1.0",
    "node-sass": "^4.13.1",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.19",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-security": "^1.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "tslint-plugin-prettier": "^2.2.0",
    "typescript": "^3.7.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "jest-extended": "^0.11.5",
    "js-logger": "^1.6.0",
    "lodash": "^4.17.15",
    "mobx": "^4.15.4",
    "mobx-react": "^6.1.8",
    "mobx-react-router": "^4.1.0",
    "mobx-utils": "^5.5.5",
    "moment": "^2.24.0",
    "react-router": "^5.1.2"
  }
}
